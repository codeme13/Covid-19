    <div class="main-banner main" id="top">
      <video autoplay muted loop id="bg-video">
          <source src="images/Covid.mp4" type="video/mp4" />
      </video>
      <div class="video-overlay header-text">
        <div class="caption">
            <h6>Get vaccinated as soon as possible</h6>
            <h2>Stay Home <i class="fa fa-home home_btn " style="color: #D83F87!important;" ></i> Stay Safe</h2>
            <div class="main-button scroll-to-section">
              <a href="https://selfregistration.cowin.gov.in/" target="_blank">Vaccinate Now</a>
             
            </div>
        </div>
    </div>

    </div>
    <div class="main_header" id="home">
      <div class="row w-100 h-100">
        <div class="col-lg-5 col-md-5 col-12 order-lg-1 order-2">
          <div
            class="
              leftside
              w-100
              h-100
              d-flex
              justify-content-center
              align-items-center
            "
          >
            <img src="images/corona2.png" width="350" height="350" />
          </div>
        </div>
        <div class="col-lg-7 col-md-7 col-12 order-lg-2 order-1">
          <div
            class="
              rightside
              w-100
              h-100
              d-flex
              justify-content-center
              align-items-center
            "
          >
            <h1>
              Let's Stay Safe & Fight Together Against Corona Virus
              <span class="corona_animate"
                ><img
                  src="images/corona-1.png"
                  alt=""
                  width="260"
                  height="150"
              /></span>
            </h1>
          </div>
        </div>
      </div>
    </div>
    <br />
    <br />

    <!-- *****api  response corona updates**********-->
    <section class="corona_update">
      <div class="mb-3">
        <h3 class="text-uppercase text-center">Covid'19 Updates</h3>
      </div>
      <div class="d-flex justify-content-around align-items-center mt-5 count_style">
        <% result.forEach(function(i) { %>
          <div>
            <h1 class="count"><%= i.data %></h1>
            <p style="font-size: 20px;"><%=i.name %></p>
          </div>
        <% }); %>
